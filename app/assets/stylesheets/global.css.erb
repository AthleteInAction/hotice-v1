.container {
	width: 980px;
	margin-left: auto;
	margin-right: auto;
}

.big_container {
	width: 1080px;
	margin-left: auto;
	margin-right: auto;
}

h1 {
font-size: 60px;
font-weight: normal;
line-height: 0.5;
color: #fff;
letter-spacing: -1px;
text-shadow: 0 1px 3px #222;
}

h2 {
	font-size: 21px;
font-weight: 300;
line-height: 1.3;
color: #fff;
text-shadow: 0 1px 3px #222;
}

.NONE {
	display: none;
}
.LEFT {
	float: left;
}
.RIGHT {
	float: right;
}
.CLEAR {
	clear: both;
}
.HALF {
	width: 50%;
}
.THIRD {
	width: 40%;
}
.TWOTHIRD {
	width: 60%;
}
.BLUE {
	color: #134B94;
}
.ORANGE {
	color: #FFAB4D !important;
}

table {
	border-spacing: 0;
    border-collapse: separate;
}
table {
	width: 100%;
}