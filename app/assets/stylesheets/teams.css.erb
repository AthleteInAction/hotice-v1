div.standard div.sep {
	margin-top: 30px;
}
div.standard div.sep:first-child {
	margin-top: 0px;
	background: #FC0 !important;
}
div.standard div.form_label {
	margin-bottom: 10px;
	color: #FFAB4D;
	text-shadow: 1px 1px 1px #222;
	margin-left: 2px;
}
div.standard div.label {
	margin-bottom: 10px;
}
div.standard div.label a {
	color: #CDCDCD;
	text-decoration: none;
	text-shadow: 1px 1px 1px #222;
}
div.standard div.label a:hover {
	color: #909090;
	text-decoration: none;
}
div.standard div.under {
	margin: 5px 5px 0 5px;
}
div.standard div.under a {
	color: #FF4E4E;
	font-size: 14px;
	text-decoration: none;
	text-shadow: 1px 1px 1px #222;
}
div.standard div.under a:hover {
	color: #DB1B1B;
	text-decoration: none;
}
div.standard div.sub_sep {
	margin-top: 10px;
}

table.team_table {
	border-spacing: 1px;
	background: #323232;
	table-layout: fixed;
	border-radius: 4px;
}
table.team_table tr {
	cursor: pointer;
}
table.team_table tr td {
	padding: 10px;
	color: #efefef;
	letter-spacing: 1px;
	text-shadow: 1px 1px 1px #222;
	font-weight: 400;
	font-size: 16px;
	background: #7B7B7B;
	width: 90px;
	text-align: center;
}
table.team_table tr:nth-child(odd) td {
	background: #5C5C5C;
}
table.team_table tr:first-child td {
	background: #50DDE8;
	color: #5E5E5E;
	font-size: 20px;
	text-shadow: 1px 1px 1px #A1FBFF;
}
table.team_table tr td:first-child {
	width: inherit;
	text-align: left;
}
table.team_table tr:hover td {
	background: #FFAB4D;
}
table.team_table tr:first-child:hover td {
	background: #50DDE8;
	cursor: default;
}

table.team_table tr:first-child td:first-child {
	border-radius: 5px 0 0 0;
}
table.team_table tr:first-child td:last-child {
	border-radius: 0px 5px 0 0;
}
table.team_table tr:last-child td:last-child {
	border-radius: 0px 0px 5px 0;
}
table.team_table tr:last-child td:first-child {
	border-radius: 0px 0px 0px 5px;
}


ul.teammates_list {
	width: 100%;
	list-style: none;
	margin: 0;
	padding: 0;
}
ul.teammates_list li {
	padding: 7px;
	background: #c0c0c0;
	color: #4A4A4A;
	text-shadow: 1px 1px 0px #E0E0E0;
}
ul.teammates_list li:nth-child(even){
	background: #c5c5c5;
}
ul.teammates_list li:last-child {
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
}

table.inner {
	border-radius: 3px;
	/*border: solid 1px #c0c0c0;*/
	table-layout: fixed;
}
table.inner tr.inner th.inner {
	text-align: left;
	vertical-align: middle;
	background: #D3D3D3;
	padding: 10px;
	color: #7B7B7B;
	font-weight: 300;
	font-size: 30px;
	text-shadow: 1px 1px 1px #F4F4F4;
	font-style: italic;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	letter-spacing: 1px;
}
table.inner tr.inner th.inner:last-child {
	text-align: right;
}
/*FF7878*/
table.inner tr.inner td.inner {
	text-align: left;
	vertical-align: middle;
	background: #B3B1B1;
	text-shadow: 1px 1px 1px #CBCBCB;
	color: #515151;
	font-size: 20px;
	padding: 12px 10px 10px 10px;
	text-shadow: 1px 1px 1px #DBDBDB;
	border-top: solid 1px #A9A9A9;
}
table.inner tr.inner:last-child td.inner {
	border-bottom-left-radius: 3px;
	border-bottom-right-radius: 3px;
}
table.inner tr.inner td.inner a.remove {
	color: #CF4B4B;
	text-decoration: none;
	font-size: 18px;
	text-shadow: 1px 1px 1px #D2D2D2;
}
table.inner tr.inner td.inner a.remove:hover {
	color: #943434;
}
table.inner tr.inner th.inner input[type="text"] {
	font-size: 14px;
	padding: 10px 8px 7px 8px;
	font-weight: normal;
	letter-spacing: 0.5px;
}
table.inner tr.inner td.inner:last-child {
	text-align: right;
}
table.inner tr.inner:nth-child(2) td.inner {
	border-top: none;
}
table.inner tr.inner:hover td.inner {
	background: #c2c2c2;
}