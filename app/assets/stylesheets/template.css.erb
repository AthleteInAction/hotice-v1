div.template_container {
	margin: 40px;
}
div.template_container div.template_container {
	margin: 0;
}

/* BREADCRUMB */
div.template_container div.breadcrumb {

}
div.template_container div.breadcrumb a {
	color: #CCC;
}
div.template_container div.breadcrumb a:hover {
	color: #FFab4a;
	text-decoration: none;
}
div.template_container div.breadcrumb span.arrow {
	color: #838383;
}
/* BREADCRUMB */

/* HEADER */
div.template_container div.head {
	display: table;
	width: 100%;
	border-collapse: separate;
	border-spacing: 0;
	background: #50DDE8;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}
div.template_container div.head div.head-item {
	display: table-cell;
	vertical-align: middle;
	text-align: right;
	margin: 0;
	padding: 10px;
	width: 1px;
	color: #262626;
	font-size: 26px;
}
div.template_container div.head div.head-item:first-child {
	text-align: left;
	width: inherit;
	background-repeat: no-repeat;
	background-position: 10px center;
	background-size: 25px;
	padding-left: 44px;
}
div.template_container div.head div.head-item.head-icon-teams:first-child {
	background-image: url(/assets/head-icon-teams.png);
}
div.template_container div.head div.head-item.head-icon-events:first-child {
	background-image: url(/assets/head-icon-events.png);
}
div.template_container div.head div.head-item.head-icon-members:first-child {
	background-image: url(/assets/head-icon-members.png);
}
div.template_container div.head div.head-item.head-icon-news:first-child {
	background-image: url(/assets/head-icon-news.png);
}

div.template_container div.head.small {
	border-top-left-radius: 6px;
	border-top-right-radius: 6px;
}
div.template_container div.head.small div.head-item {
	background-repeat: no-repeat;
	background-position: 10px center;
	background-size: 19px;
	padding: 6px;
	padding-left: 36px;
	padding-top: 7px;
	font-size: 18px;
}
/* HEADER */